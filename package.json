{
  "name": "react-ui-nodejs-starter",
  "version": "1.3.5",
  "scripts": {
    "build": "gulp webpack-prod",
    "test": "jest --env=jsdom",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "jest --watch",
    "start": "node src/server/server.js",
    "dev": "gulp dev",
    "flow:start": "flow"
  },
  "engines": {
    "node": "10.x"
  },
  "browserslist": [
    "Firefox ESR",
    "ie 11",
    "last 2 chrome versions",
    "last 2 samsung versions"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "1.18.2",
    "cfenv": "1.0.4",
    "config": "3.2.2",
    "cors": "2.8.4",
    "eureka-server-starter": "3.1.9",
    "express": "4.16.3",
    "faker": "^4.1.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "material-ui-upload": "^1.2.1",
    "moment": "2.22.2",
    "pkv-express-middleware": "0.0.2",
    "react-select": "^2.4.1",
    "request": "2.87.0",
    "s3": "^4.4.0",
    "uuid": "3.2.1",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "natives": "^1.1.6",
    "@material-ui/core": "3.4.0",
    "@material-ui/icons": "3.0.1",
    "aims-reusable-react-components": "1.1.58",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.5",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "browser-sync": "2.24.6",
    "connect-browser-sync": "^2.1.0",
    "connect-history-api-fallback": "1.5.0",
    "webpack": "4.20.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "0.28.4",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "es6-promise": "^4.2.5",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "3.1.2",
    "eventsource-polyfill": "^0.9.6",
    "expose-loader": "0.7.4",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "^0.70.0",
    "gulp-cached": "1.1.1",
    "gulp-eslint": "4.0.0",
    "gulp-jshint": "2.0.4",
    "gulp-multi-process": "^1.3.1",
    "gulp-nodemon": "2.2.1",
    "gulp-react": "3.1.0",
    "history": "4.6.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "^22.4.3",
    "jest-alias-preprocessor": "^1.1.1",
    "jest-cli": "21.2.1",
    "jest-runtime": "^22.4.2",
    "jquery": "^3.3.1",
    "jshint": "2.9.5",
    "jshint-stylish": "2.2.1",
    "json-loader": "0.5.4",
    "lodash": "4.17.10",
    "material-ui-pickers": "1.0.0-rc.17",
    "mui-datatables": "^2.0.0-beta-32",
    "notistack": "^0.3.7",
    "prop-types": "15.5.10",
    "react": "16.12.0",
    "react-datepicker": "^1.6.0",
    "react-dom": "16.12.0",
    "react-gemini-scrollbar": "2.3.2",
    "react-hot-loader": "4.3.11",
    "react-logger": "^1.1.0",
    "react-raw-html": "0.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-table": "^6.8.6",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-form-material-ui": "5.0.0-beta.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.3.0",
    "style-loader": "0.18.2",
    "transfer-webpack-plugin": "0.1.4",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "0.5.8",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-server": "3.1.9",
    "webpack-md5-hash": "0.0.6",
    "webpack-module-hot-accept": "^1.0.5",
    "webpack-stream": "^5.1.0",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.8"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "env",
          "flow",
          "react",
          "stage-1"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ],
          "flow",
          "react",
          "stage-1"
        ],
        "plugins": [
          "react-hot-loader/babel",
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "flow",
          "stage-1"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "scriptPreprocessor": "./tools/testPreprocessor.js",
    "setupFiles": [
      "./tools/shimSetup.js",
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules",
      "!<rootDir>/dist"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!echarts-for-react)"
    ]
  }
}
